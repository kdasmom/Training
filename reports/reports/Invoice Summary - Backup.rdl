  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="invoice_ref">
          <DataField>invoice_ref</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="invoice_amount">
          <DataField>invoice_amount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="vendor_name">
          <DataField>vendor_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="vendor_id_alt">
          <DataField>vendor_id_alt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="property_name">
          <DataField>property_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="property_id_alt">
          <DataField>property_id_alt</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>NP</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT
	TOP 50
	i.invoice_ref,
	ISNULL((
		SELECT SUM(ii.invoiceitem_amount + ii.invoiceitem_salestax + ii.invoiceitem_shipping)
		FROM invoiceitem ii
		WHERE ii.invoice_id = i.invoice_id
	), 0) AS invoice_amount,
	v.vendor_name,
	v.vendor_id_alt,
	p.property_name,
	p.property_id_alt
FROM invoice i
	INNER JOIN property p ON i.property_id = p.property_id
	INNER JOIN vendorsite vs ON i.paytablekey_id = vs.vendorsite_id
	INNER JOIN vendor v ON vs.vendor_id = v.vendor_id
ORDER BY v.vendor_name, p.property_name</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
</Report>